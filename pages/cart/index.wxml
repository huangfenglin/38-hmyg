<view class="address_view">
  <!-- 当没有收货地址的时候就显示按钮 -->
  <button wx:if="{{!address.userName}}" bindtap="handleFinalGet">获取收货地址</button>
  <!-- 有收货地址了 就显示 收货地址即可 -->
  <view wx:else>{{address.userName}}</view>
</view>


<!-- 购物车 开始 -->
